            "line-height": "46px",
            "color": "@color-swatch4",
            "letter-spacing": "0px"
        },
        "font2": {
            "font-family": "\"Poppins\", \"Montserrat\", \"Trebuchet MS\", Helvetica, sans-serif",
            "font-size": "30px",
