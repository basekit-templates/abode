            "letter-spacing": "0px",
            "background-color": "@color-swatch7",
            "background-color-hover": "@color-swatch7"
         },
        "font7": {
            "font-family": "\"Poppins\", \"Montserrat\", \"Trebuchet MS\", Helvetica, sans-serif",
            "font-size": "16px",
            "font-weight": "300",
            "line-height": "26px",
            "color": "@color-swatch7",
            "letter-spacing": "0px",
            "background-color": "@color-swatch3",
