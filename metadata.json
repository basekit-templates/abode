            "color": "@color-swatch4",
            "letter-spacing": "0px"
        },
        "font3": {
            "font-family": "\"Poppins\", \"Montserrat\", \"Trebuchet MS\", Helvetica, sans-serif",
            "font-size": "26px",
            "font-weight": "700",
            "line-height": "36px",
            "color": "@color-swatch4",
            "letter-spacing": "0px"
        },
        "font4": {
            "font-family": "\"Poppins\", \"Montserrat\", \"Trebuchet MS\", Helvetica, sans-serif",
