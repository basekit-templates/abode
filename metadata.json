        },
        "font8": {
            "font-family": "\"Poppins\", \"Montserrat\", \"Trebuchet MS\", Helvetica, sans-serif",
            "font-size": "36px",
            "font-weight": "400",
            "line-height": "44px",
            "color": "@color-swatch7",
            "letter-spacing": "0px"
        },
        "font9": {
            "font-family": "\"Lato\", Geneva, Tahoma, sans-serif",
            "font-size": "20px",
            "font-weight": "400",
            "line-height": "27px",
            "color": "@color-swatch1",
            "letter-spacing": "0px"
        },
        "font10": {
            "font-family": "\"Poppins\", \"Montserrat\", \"Trebuchet MS\", Helvetica, sans-serif",
            "font-size": "32px",
            "font-weight": "700",
            "line-height": "36px",
            "color": "@color-swatch4",
            "letter-spacing": "0px"
        }
