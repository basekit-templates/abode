{
    "name": "abode",
    "widgetVersion":2,
    "thumbnail": "thumbnail.png",
    "screenshot": "abode-screenshot.jpg",
    "templateType":"standard",
    "pageTypes": ["home", "blogpost"],
    "images":[
        "images/image-1.jpg",
        "images/gallery/img1.jpg",
        "images/gallery/img2.jpg",
        "images/gallery/img3.jpg",
        "images/gallery/img4.jpg",
        "images/logo.png"
    ],
    "colorSwatches":
    {
        "Swatch 1": {
            "color1":"#ffffff",
            "color2":"#24bb91",
            "color3":"#141426",
            "color4":"#141426",
            "color5":"#75757b",
            "color6":"#b7b7b7",
            "color7":"#ffffff"
        }
    },
    "fontSwatch": {
